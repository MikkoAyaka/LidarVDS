<!-- 色彩、刷子相关的常量都放这里 -->
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="Brush-NavBarBackground" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0.0" Color="#6DA6F6"/>
        <GradientStop Offset="1.0" Color="#6DA6F6"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="Brush-FastUseBackground" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Offset="0.0" Color="#73FCF7"/>
        <GradientStop Offset="1.0" Color="#53BAEE"/>
    </LinearGradientBrush>
    <Brush x:Key="Brush-SecondaryBackground">#F4F7FE</Brush>
    <Brush x:Key="Brush-PrimaryBackground">White</Brush>
    <Brush x:Key="Brush-NavButtonText">#2A7BF6</Brush>
    <Brush x:Key="Brush-DocButtonText">#1A1A1A</Brush>
    <Brush x:Key="Brush-DocButtonBackground">#EEEEEE</Brush>
    <Brush x:Key="Brush-DocButtonCheckedBorder">#0A6CF5</Brush>
    
    <SolidColorBrush x:Key="NavBarThemeColor" Color="#6DA6F6"/>
    <SolidColorBrush x:Key="Background" Color="#5EC603"/>
    <SolidColorBrush x:Key="Color1" Color="#9DF252"/>
    <SolidColorBrush x:Key="Color2" Color="#9DF252"/>
    <SolidColorBrush x:Key="Color3" Color="#5EC603"/>
</ResourceDictionary>